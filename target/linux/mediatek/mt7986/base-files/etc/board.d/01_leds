#!/bin/sh

. /lib/functions.sh
. /lib/functions/leds.sh
. /lib/functions/uci-defaults.sh

board=$(board_name)

board_config_update

case $board in
xiaomi,redmi-router-ax6000|\
 	xiaomi,redmi-router-ax6000-uboot)
	ucidef_set_led_default "status" "status" "rgb:status" "1"
	ucidef_set_led_netdev "wan" "wan" "rgb:network" "eth1"
	;;
*netcore,n60*)
	ucidef_set_led_default "blue" "BLUE" "n60:blue" "1"
	ucidef_set_led_default "red" "RED" "n60:red" "0"
	;;
esac

board_config_flush

exit 0
